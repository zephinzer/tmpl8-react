{
  "name": "tmpl8-react",
  "scripts": {
    "build": "webpack --display-entrypoints --progress --config ./config/webpack.config.js",
    "deploy-pm2": "./scripts/.deploy-pm2",
    "deploy-docker-dev": "./scripts/.deploy-docker-dev",
    "deploy-docker-prd": "./scripts/.deploy-docker-prd",
    "unbuild": "./scripts/.unbuild",
    "start": "if [[ $NODE_ENV = 'prd' ]]; then node index.js; else nodemon index.js; fi;",
    "eslint": "./node_modules/.bin/eslint .",
    "eslint-fix": "./node_modules/.bin/eslint . --fix",
    "karma": "./node_modules/.bin/karma start ./config/karma.config.js --single-run",
    "karma-watch": "./node_modules/.bin/karma start ./config/karma.config.js",
    "mocha": "istanbul cover _mocha -- -R text -R mochawesome --reporter-options reportDir=assets/coverage/mocha,reportFilename=index.html -r test/index.js --recursive test",
    "mocha-watch": "npm run mocha -- -w",
    "nsp": "nsp check"
  },
  "dependencies": {
